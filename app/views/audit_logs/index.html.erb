<table class="table table-striped table-hover table-sm">
  <thead>
    <tr>
      <th>
        #
      </th>
      <th>
        Employee
      </th>
      <th>
        Week Starting
      </th>
      <th>
        Confirmed At
      </th>
      <th>
        Status
      </th>
    </tr>
  </thead>
  <tbody id="audit_logs">
    <%= render @audit_logs %>
  </tbody>
</table>

<div class="d-flex align-items-center">
  <div class="m-auto" id="paginator">
    <%= paginate @audit_logs, remote: true %>
  </div>
</div>

